{
  "name": "crypto-lite",
  "version": "0.0.7",
  "stability": 1,
  "license": "MIT",
  "author": {
    "name": "Lauri Rooden",
    "email": "lauri@rooden.ee"
  },
  "description": "Standard and secure cryptographic algorithms for browser",
  "keywords": [
    "crypto",
    "sha1",
    "sha256",
    "hmac",
    "pbkdf2"
  ],
  "main": "index.js",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/litejs/crypto-lite.git"
  },
  "bugs": {
    "url": "https://github.com/litejs/crypto-lite/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "testman": "*"
  },
  "buildman": {
    "dist/crypto-lite-min.js": "index.js",
    "dist/sha1.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "sha256|HMAC|PBKDF2"
    },
    "dist/sha1-hmac-pbkdf2.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "sha256"
    },
    "dist/sha256.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "sha1|HMAC|PBKDF2"
    },
    "dist/sha256-hmac-pbkdf2.js": {
      "devel": true,
      "input": "index.js",
      "toggle": "sha1"
    }
  },
  "testling": {
    "files": "tests/run.js",
    "browsers": [
      "ie/6..10",
      "ff/3",
      "ff/4",
      "ff/5",
      "ff/20",
      "ff/25",
      "chrome/4",
      "chrome/10",
      "chrome/20",
      "chrome/30",
      "chrome/31",
      "opera/10",
      "opera/11",
      "opera/11.6",
      "opera/12",
      "opera/16",
      "safari/4..6",
      "android-browser/4.2",
      "iphone/6",
      "ipad/6"
    ]
  },
  "gitHead": "3c27f9750bfd24898b56e7aa0eb602d8a0819f1f",
  "homepage": "https://github.com/litejs/crypto-lite",
  "_id": "crypto-lite@0.0.7",
  "_shasum": "509c413eb0eca3a57c3853cba3ef558e2e175f87",
  "_from": "crypto-lite@",
  "_npmVersion": "1.4.25",
  "_npmUser": {
    "name": "lauriro",
    "email": "lauri@rooden.ee"
  },
  "maintainers": [
    {
      "name": "lauriro",
      "email": "lauri@rooden.ee"
    }
  ],
  "dist": {
    "shasum": "509c413eb0eca3a57c3853cba3ef558e2e175f87",
    "tarball": "http://registry.npmjs.org/crypto-lite/-/crypto-lite-0.0.7.tgz"
  },
  "directories": {},
  "_resolved": "https://registry.npmjs.org/crypto-lite/-/crypto-lite-0.0.7.tgz"
}
